page{background: #fff;}
.item-title{
	position:relative;
	padding:38rpx 50rpx 38rpx 62rpx;
	font-size:34rpx;
	color:#000;
}
.item-title:before{
	content:" ";
	position:absolute;
	left: 39rpx;
	top:50%;
	bottom:40rpx;
	height: 40rpx;
	width: 11rpx;
	border-radius: 11rpx;
	background-color: #5394ff;
	-webkit-transform-origin: 0 0;
	transform-origin: 0 0;
	-webkit-transform: translateY(-50%);
	transform: translateY(-50%);
}
.item-title:after{
	content:" ";
	position:absolute;
	bottom:0;
	right:0;
	color:#d9d9d9;
	left:45rpx;
	border-bottom: 1rpx dashed #979797;
}
.item-box{
	padding: 0 58rpx 20rpx 62rpx;
}
.item-cell{
	padding: 20rpx 0 8rpx 0;
	font-size:30rpx;
	color:#000000;
}
.item-descr{
	padding: 0 5rpx 5rpx 0;
	font-size:30rpx;
	color:#7a7a7a;
}