/* pages/index/index.wxss */
.manageList{
   height: 190rpx;
   background: #fff;
   margin:20rpx 20rpx 20rpx 20rpx;
   border-radius:20rpx;
   box-shadow: 2px 1px 8px #dddbdb;
   position: relative
}
.confirm{
  width: 30%;
  position: absolute;
  top: 50%;
  left: 0;
  right: 0;
  margin: 0 auto;
  margin-top: -60rpx;
  z-index: 1;
  background: #fff;
  border-radius: 2rpx;
  overflow: hidden;
}
.confirm view{
   height: 60rpx;
   line-height:60rpx;
   text-align: center;
   font-size: 24rpx;
  
}
.confirm .delete{
  color: #dd1e1e;
   border-bottom: 1px solid #ccc;
}
.confirm .cancel{
   color: #000
}
.manageList .checked{
 width:50rpx;
 height:50rpx;  
 position: absolute;
 top: 95rpx;
 margin-top: -25rpx;
 left: 50rpx
}

.manageList .list-right{
   float: right;
   margin:2rpx 120rpx 0rpx 0rpx;
}
.manageList .list-right .card-img{
 width:30rpx;
 height:22rpx;
 margin-right: 15rpx;
}
.manageList .list-right .card{
 font-size: 26rpx;
 color: #7A7A7A;
 margin-top: 10rpx;
 }
 .manageList .list-right .phone-img{
 width:30rpx;
 height:22rpx;
 margin-right: 15rpx;
 }
 .manageList .list-right .phone{
 font-size: 26rpx;
 color: #7A7A7A;
 margin-top: 10rpx;
 }
.manageList .list-right .essential-i .name{
 font-size: 34rpx;
 color: #000;
 margin-right: 40rpx;
 }
 .manageList .list-right .essential-i .sex{
 font-size: 24rpx;
 color: #7A7A7A;
 margin-right: 20rpx;
 }
  .manageList .list-right .essential-i .age{
 font-size: 24rpx;
 color: #7A7A7A;
 margin-right: 20rpx;
 }


