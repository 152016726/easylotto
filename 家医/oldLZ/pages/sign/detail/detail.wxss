page {
  background-color: #f3f3f3;
  padding-bottom: 240rpx;
}
.block {
  background-color: #fff;
  margin-bottom: 15rpx;
  font-size: 32rpx;
	letter-spacing: 3rpx;
	color: #545454;
}
.block .line {
  padding: 30rpx;
}
.block .line:not(:last-child) {
  border-bottom: 1rpx solid rgba(229, 229, 229, 0.55);
}
.block1 .team {
  padding: 30rpx 100rpx 30rpx 203rpx;
  position: relative;
  padding-left: 203rpx;
  border-bottom: 1rpx solid #e8e8e8;
}
.block1 .team image {
  width: 140rpx;
  height: 140rpx;
  position: absolute;
  top: 36rpx;
  background-color: #e5e5e5;
  left:30rpx;
  border-radius: 11rpx;
  border: 1rpx solid #b5b5b5;
}
.block1 .team .right-ico {
  position:absolute;
  top:50%;
  right:30rpx;
  margin-top:-8rpx;
}
.block1 .team .content {
  position:relative;
}
.block1 .team .content .name {
	font-size: 36rpx;
	color: #3a3a3a;
}
.block1 .team .content .description {
  font-size: 27rpx;
	color: #979797;
  padding-bottom: 10rpx;
  display:block;
  word-break:keep-all;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.block1 .team .content .count {
  font-size: 25rpx;
	color: #999999;
  margin-top:8rpx;
}
.block1 .team .content .count .sign-num {
  margin-right:15rpx;
  display: inline-block;
}
.block1 .team .content .count .service-num {
  display: inline-block;
}
.block1 .team .content .count .sign-num .pen-circle-ico,
.block1 .team .content .count .service-num .love-circle-ico {
  position:relative;
  top:5rpx;
  margin-right: 5rpx
}
.block1 .indications{
  position: relative;
  padding: 20rpx;
  padding-left: 70rpx;
	font-size: 28rpx;
	color: #545454;
}
.block1 .indications .indications-ico {
  position: absolute;
  left:20rpx;
  top:27rpx;
}
.block1 .indications .content {
  font-size: 27rpx;
	color: #979797;
  padding-bottom: 5rpx;
  padding-top: 5rpx;
  display:block;
  word-break:keep-all;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.block2 .header {
  border-bottom: 1rpx solid rgba(229, 229, 229, 0.55);
  padding: 12rpx 30rpx;
  font-size: 32rpx;
	letter-spacing: 3rpx;
	color: #313131;
}
.block2 .content {
  
}
.block2 .content .box {
    padding: 20rpx 30rpx;
    display: inline-block;
    text-align: center;
}
.block2 .content .box image {
  width: 100rpx;
  height: 100rpx;
  background-color: #e5e5e5;
  border-radius: 50%;
}
.block2 .content .box .name {
	font-size: 30rpx;
	letter-spacing: 3rpx;
	color: #545454;
}
.block2 .content .box .tag {
  font-size: 22rpx;
	letter-spacing: 2rpx;
	color: #ffffff;
  background-color: #f67d25;
	border-radius: 20rpx;
  display:inline-block;
  padding:3rpx 16rpx;
}
.block4 {
  padding: 30rpx;
}
.block4 .info {
  font-size: 30rpx;
	letter-spacing: 3rpx;
	color: #696969;
}
.block5 .header {
  border-bottom: 1rpx solid rgba(229, 229, 229, 0.55);
  padding: 12rpx 30rpx;
}
.block5 .content {
	font-size: 30rpx;
	letter-spacing: 0rpx;
	color: #999999;
  padding: 20rpx 30rpx;
  text-align: justify;
}
.bottom-block {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  box-shadow:0rpx -2rpx 10rpx 0rpx rgba(0, 0, 0, 0.11);
}
.bottom-block .sign-konw {
  font-size: 24rpx;
  padding: 30rpx;
}
.bottom-block .sign-konw .question-white-ico {
  position:relative;
  top:9rpx;
  margin-left:10rpx;
}
.bottom-block .next-btn {
	font-size: 34rpx;
	letter-spacing: 3rpx;
	color: #ffffff;
  text-align: center;
  padding: 50rpx 0;
  line-height: 1;
  background: -webkit-linear-gradient(left,#1ac2f9,#0099ff);
  background: -o-linear-gradient(right,#1ac2f9,#0099ff);
  background: -moz-linear-gradient(right,#1ac2f9,#0099ff);
  background: linear-gradient(to right,#1ac2f9,#0099ff);
}
.next-btn[disabled] {
  opacity: 0.5;
  color: #fff !important;
}