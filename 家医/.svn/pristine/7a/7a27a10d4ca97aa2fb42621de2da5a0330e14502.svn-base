.page-content_unfixed{padding-bottom: 240rpx;}
/*滚动栏*/
.scroll-view{
	height: 284rpx;
	white-space: nowrap;
	display: -webkit-flex;
 	display: flex;
	align-items: center;
  	-webkit-align-items: center;
} 
.scroll-box{
	display: inline-block;
	width:421rpx;
	padding:10rpx 18rpx 11rpx;
	margin:0 16rpx;
	color:#b2b2b2;
    font-size:26rpx;
    border-radius:20rpx;
	background:#fff;
}
.scroll-view radio[checked]+.scroll-box{
	display: inline-block;
	width:421rpx;
	padding:16rpx 18rpx 17rpx;
	margin:0 16rpx;
	color:#fff;
	font-size:26rpx;
	border-radius:20rpx;
	background-image:linear-gradient(65deg, #ffba66 6%, #fba13c 100%);
	box-shadow:0rpx 4rpx 10rpx 0rpx rgba(255,193,138,0.70);
}
.flex-between{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	justify-content: space-between;
}
.box-title{
	width:380rpx;
	white-space:pre-line;
	color: #b2b2b2;
	font-size: 32rpx;
	font-weight: 600;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.lz-choosen-ico {
  position:relative;
  top:7rpx;
}

.scroll-view radio[checked]+.scroll-box .box-title{color: #fff;}
.scroll-block{
	display: inline-block;
	min-width:104rpx;
	padding:4rpx 12rpx;
	margin:14rpx 0 24rpx;
	text-align: center;
	border:1rpx solid #b2b2b2;
	border-radius:100rpx;
}
.scroll-view radio+.scroll-box .lz-choosen-ico {
    display: inline-block;
    background: url("http://cda.heyuht.com:8181/image/small/lzico1.png") no-repeat;
    background-position:-580rpx -161rpx;
    background-size:750rpx auto;
    width:40rpx;
    height:40rpx;
}
.scroll-view radio[checked]+.scroll-box .scroll-block{border:1rpx solid #fff;}
/*.scroll-view radio+.scroll-box .lz-choosen-ico,*/
.scroll-view radio+.scroll-box .lz-write-white-ico,
.scroll-view radio[checked]+.scroll-box .lz-write-grey-ico,
.scroll-view radio+.scroll-box .lz-calendar-white-ico,
.scroll-view radio[checked]+.scroll-box .lz-calendar-grey-ico{
	display: none;
}
.scroll-view radio[checked]+.scroll-box .lz-choosen-ico,
.scroll-view radio+.scroll-box .lz-write-grey-ico,
.scroll-view radio[checked]+.scroll-box .lz-write-white-ico,
.scroll-view radio+.scroll-box .lz-calendar-grey-ico,
.scroll-view radio[checked]+.scroll-box .lz-calendar-white-ico{
	display: inline-block;
}
.scroll-view radio[checked]+.scroll-box .lz-choosen-ico {
  background:url("http://cda.heyuht.com:8181/image/small/lzico2.png") no-repeat;
  background-position:-418rpx -2rpx;
  background-size:750rpx auto;
  width:38rpx;
  height:40rpx;
  display:inline-block;
}
/*中间信息栏*/
.wrap-edit_middle{
	background-color: #fff;
	padding:0 0 30rpx 40rpx;
}
.wrap-line1{
	position: relative;
	padding:36rpx 40rpx;
	font-size: 34rpx;
	font-weight: 600;
	color:#000;
	border-bottom: 1rpx dashed #979797;
}
.title-block:before{
    content: " ";
    position: absolute;
    left:10rpx;
    top:50%;
    width:22rpx;
    height:40rpx;
    background:#5394ff;
    border-radius:8rpx;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleX(0.5) translateY(-50%); 
    transform: scaleX(0.5) translateY(-50%);
}
button:after{border:0;}
button{
	background-color: #fff;
	padding-left: 10rpx;
	padding-right: 10rpx;
}
.wrap-line1 button{
	margin-left: 0;
	margin-right: 0;
	line-height: inherit;
}
.wrap-line2{
	justify-content: flex-start;
	padding:25rpx 10rpx;
}
.wrap-line3{
	justify-content: flex-start;
	padding:0rpx 20rpx 0 10rpx;
}
/*地址栏*/
.wrap-edit_bottom{
	width:690rpx;
	margin:34rpx auto;
	border:1rpx soild #e8e8e8;
	border-radius:20rpx;
	box-shadow: 0 2rpx 10rpx 0 rgba(0,0,0,0.16);
	background-color: #fff;
}
.wrap-title{
	padding:25rpx 34rpx;
	font-size:32rpx;
	font-weight: 600;
	color:#000;
	line-height: 70rpx;
}
.wrap-content{
	line-height:1.5em;
	padding:10rpx 36rpx 16rpx;
}
.wrap-content .textarea{
	height: 170rpx;
}
/*底边栏*/
/*底部固定按钮*/
.page-bottom_fixed{
    position:fixed;
    left:0;
    right: 0;
    bottom:0;
    height:180rpx;
    padding:30rpx 35rpx;
    font-size:28rpx;
    color:#7a7a7a;
    background:#ffffff;
    box-shadow:0 -2rpx 10rpx 0 rgba(0,0,0,0.16);
    z-index: 1000;
    opacity: 0.9;
}
.page-bottom_fixed .agree-checkbox {
  display:block;width: 100%
}
.page-bottom_fixed .checkbox{
	line-height: 38rpx;
  display:block;width: 100%;
}
.f-c_warn{ 
	color:#ff8801;
	text-decoration: underline;
}
.weui-btn_info{
    color:#fff;
    font-size: 34rpx;
    background:#5394ff;
    border-radius:100rpx;
}
.weui-btn_info:visited,
.weui-btn_info:active{color:#d0d0d0;}
.weui-btn_info[disabled]:not([type]) {
    opacity: 0.5;
    background:#5394ff;
    color:#fff;
}
.sign-wrap .weui-mask{
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0,0,0,0.6);
}
.sign-wrap .dialog-content{
  position: fixed;
  z-index: 2000;
  top: 50%;
  left: 50%;
  border-radius: 3px;
  overflow: hidden;
  -webkit-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
}
.sign-wrap .dialog-header{
  position:relative;
  width: 636rpx;
  height: 282rpx;
  background-color: #09f;
}
.sign-wrap .dialog-header_title{
  padding: 34rpx 0 0;
  font-size: 35rpx;
  color:#fff;
  text-align: center;
}
.sign-wrap .dialog-header_bg{
  position:absolute;
  margin:0 auto;
  bottom: -1rpx;
  left:50%;
  transform: translateX(-50%);
}
.sign-wrap .dialog-body{
  font-size: 32rpx;
  color: #3f3f3f;
  background:#fff;
}
.sign-wrap .dialog-body_title{
  padding: 15rpx 33rpx;
}
.sign-wrap .avator-img_dialog{
  width:141rpx;
  height:141rpx;
  margin-right: 22rpx;
  border:1rpx solid #b5b5b5;
  border-radius: 12rpx;
  vertical-align: middle;
}
.sign-wrap  .dialog-main{padding: 15rpx 33rpx; }
.sign-wrap .dialog-btn_area{padding:30rpx 40rpx; justify-content: space-between;  }
.sign-wrap .dialog-box_close{padding-top:30rpx;}
.sign-wrap .dialog-delete{
  position:relative;
  left: 50%;
  transform: translateX(-50%);
  z-index: 3000;
}
.sign-wrap .weui-btn{margin:0; padding:0;}
.sign-wrap .weui-btn+.weui-btn{margin-top: 0;}
.sign-wrap .weui-btn_normal{background-color: #09f; font-size: 28rpx;color:#fff;border-radius: 35rpx;}
.sign-wrap .weui-btn_normal:visited{color:rgba(255, 255, 255,0.4); background-color: #0184dc;}
.sign-wrap .weui-btn_normal:active{color:rgba(255, 255, 255,0.4); background-color: #0184dc;}
.sign-wrap .weui-btn_base{background-color: #fff;font-size: 28rpx;color: #999; border:1px solid #999;border-radius: 35rpx;}
.sign-wrap .weui-btn_base:after{border-width: 0}
.sign-wrap .weui-btn_base:visited{color:#bababa;border:1px solid #bababa;}
.sign-wrap .weui-btn_base:active{color:#bababa;border:1px solid #bababa;}
.sign-wrap .weui-btn_md{width:235rpx;height: 70rpx;line-height: 70rpx;text-align:center;}
.service-block {
  margin-bottom: 20rpx;
}
.service-block .service-line {
  font-size: 28rpx;
  padding: 30rpx;
  background-color: #fff;
}
.service-block .service-line:not(:last-child) {
  border-bottom: 1px #e5e5e5 solid;
}
.service-block .service-line .service-header {
  margin-bottom:20rpx;
}