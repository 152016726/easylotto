page {
  background-color: #f3f3f3;
  padding-bottom:240rpx;
  box-sizing: border-box
}
.block {
  background-color: #fff;
  margin-bottom: 15rpx;
  font-size: 32rpx;
	letter-spacing: 3rpx;
	color: #545454;
}
.block .line {
  padding: 30rpx;
}
.block .line:not(:last-child) {
  border-bottom: 1rpx solid rgba(229, 229, 229, 0.55);
}
.linkman-block {
  position: relative;
  padding-right: 70rpx;
}
.linkman-block .pen-ico {
  position: absolute;
  right: 35rpx;
  margin-top: -15rpx;
  top: 50%;
}
.linkman-block .info {
  padding:25rpx 35rpx 35rpx;

}
.linkman-block .info .line1 {
  padding:0 35rpx 30rpx;
}
.linkman-block .info .line1 >text:nth-child(1) {
  font-size: 36rpx;
	letter-spacing: 0rpx;
	color: #525252;
  margin-right: 40rpx;
}
.linkman-block .info .line1 >text:nth-child(2) {
  font-size: 30rpx;
	letter-spacing: 0rpx;
	color: rgba(82, 82, 82, 0.5);
}
.linkman-block .info .line2 {
  padding-left: 35rpx;
  position: relative;
  font-size: 28rpx;
	color: #999999;
}
.linkman-block .info .line2 .location-line-ico {
  position:absolute;
  left:-1rpx;
  top:5rpx;
}

.block1 .team {
  padding: 30rpx 100rpx 30rpx 203rpx;
  position: relative;
  padding-left: 203rpx;
  border-bottom: 1rpx solid #e8e8e8;
}
.block1 .team image {
  width: 140rpx;
  height: 140rpx;
  position: absolute;
  top: 36rpx;
  background-color: #e5e5e5;
  left:30rpx;
  border-radius: 11rpx;
  border: 1rpx solid #b5b5b5;
}
.block1 .team .right-ico {
  position:absolute;
  top:50%;
  right:30rpx;
  margin-top:-8rpx;
}
.block1 .team .content {
  position:relative;
}
.block1 .team .content .name {
	font-size: 36rpx;
	color: #3a3a3a;
}
.block1 .team .content .description {
  font-size: 27rpx;
	color: #979797;
  padding-bottom: 10rpx;
  display:block;
  word-break:keep-all;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.block1 .team .content .count {
  font-size: 25rpx;
	color: #999999;
  margin-top:8rpx;
}
.block1 .team .content .count .sign-num {
  margin-right:15rpx;
  display: inline-block;
}
.block1 .team .content .count .service-num {
  display: inline-block;
}
.block1 .team .content .count .sign-num .pen-circle-ico,
.block1 .team .content .count .service-num .love-circle-ico {
  position:relative;
  top:5rpx;
  margin-right: 5rpx
}
.block1 .indications{
  position: relative;
  padding: 20rpx;
  padding-left: 70rpx;
	font-size: 28rpx;
	color: #545454;
}
.block1 .indications .indications-ico {
  position: absolute;
  left:20rpx;
  top:27rpx;
}
.block1 .indications .content {
  font-size: 27rpx;
	color: #979797;
  padding-bottom: 5rpx;
  padding-top: 5rpx;
  display:block;
  word-break:keep-all;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.block6 .header {
