  padding: 12rpx 30rpx;
  font-size: 32rpx;
	letter-spacing: 3rpx;
	color: #313131;
}
.block6 .content {
  
}
.block6 .content .box {
    padding: 20rpx 30rpx;
    display: inline-block;
