.page-wrap {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding-top: 20rpx;
}
.panel-box:first-child{
    margin-top: 0;
}
.panel-box{
    position: relative;
    width:630rpx;
    /*height:201rpx;*/
    padding: 20rpx 33rpx 20rpx 37rpx;
    margin:30rpx auto 0;
    font-size: 28rpx;
    color: #7a7a7a;
    background:#ffffff;
    box-shadow:4rpx 4rpx 10rpx 0 rgba(0,0,0,0.06);
    border-radius:20rpx;
    overflow: hidden;
}
.panel-box .name{
    font-size:38rpx;
    color:#000000;
}
.panel-box .item-tab{
    display: inline-block;
    height: 28rpx;
    min-width: 50rpx;
    padding:5rpx 20rpx;
    margin-left:20rpx;
    line-height: 28rpx;
    font-size:28rpx;
    color: #ff8801;
    text-align: center;
    border:2rpx solid #ff8801;
    border-radius:28rpx;
}
.panel-box .lz-location-grey-ico{
    margin-right: 10rpx;
}
.panel-box .line-cell{
    margin: 19rpx 0 2rpx;
}
.panel-box .report-state{
    font-size:30rpx;
    color: #ff8801;
}
.panel-box .patch-right_org{
    width:179rpx;
    height: 200rpx;
    position: absolute;
    right:-30rpx;
    bottom:118rpx;
    background-color:#fba559;
    border-radius: 50%;
}
.panel-box .patch-icon_face{
    position: absolute;
    top:20rpx;
    right: 33rpx;
    color: #fff;
    font-size: 30rpx;
}
.panel-box .patch-right_pink{
    width:179rpx;
    height: 200rpx;
    position: absolute;
    right:-30rpx;
    bottom:118rpx;
    background:#f07499;
    border-radius: 50%;
}