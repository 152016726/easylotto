.btn-area{padding-top:65rpx;}
/*按钮*/
.weui-btn_info{
    color:#fff;
    font-size: 34rpx;
    background:#5394ff;
    border-radius:100rpx;
}
.weui-btn_info:visited,
.weui-btn_info:active{color:#d0d0d0;}
.weui-btn_info[disabled]:not([type]) {
    opacity: 0.5;
    background:#5394ff;
    color:#fff;
}
button:after{border:0;}
button{
    background-color: #fff;
    padding-left: 10rpx;
    padding-right: 10rpx;
}
