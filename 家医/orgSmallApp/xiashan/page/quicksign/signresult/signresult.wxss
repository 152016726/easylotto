/* page/quicksign/signresult/signresult.wxss */
.yt-wrap .flex-between{
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    justify-content: space-between;
}
/*顶部信息栏*/
.img-square_lg{
	box-shadow:4rpx 4rpx 20rpx 0rpx rgba(0,0,0,0.06);
	border-radius:10rpx;
	width:170rpx;
	height:200rpx;
}
.topBox{
	padding:43rpx 31rpx 37rpx;
	background-color: #fff;
	border-bottom: 1rpx dashed #979797; 
}
.flex-item{
	flex:1;
	-webkit-flex:1;
}
.textBox_org{
	padding:3rpx 20rpx;
	font-size:28rpx;
	color:#ff8801;
	letter-spacing:-0.68rpx;
	border:2rpx solid #ff8801;
	border-radius:100rpx;
}
.text-ellipsis{
	width: 340rpx;
	display: inline-block;
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
}
.text-ellipsis_lg{width: 410rpx;}
/*团队信息栏*/
.middleBox{
	margin:18rpx 25rpx 25rpx;
	padding:14rpx 36rpx 25rpx;
	background:#5394ff;
	box-shadow:0rpx 2rpx 10rpx 0rpx rgba(0,0,0,0.16);
	border-radius:20rpx;
	font-size: 26rpx;
	color:#fff;
}
.textBox_yellow{
	padding:3rpx 20rpx;
	font-size:26rpx;
	color:#f7ff88;
	letter-spacing:-0.63rpx;
	border:2rpx solid #f7ff88;
	border-radius:100rpx;
}
.textBox_yellow+.textBox_yellow{
	margin-left: 30rpx;
}
/*中间信息栏*/
.wrap-edit_middle{
	background-color: #fff;
	padding:0 0 30rpx 40rpx;
}
.wrap-line1{
	position: relative;
	padding:26rpx 40rpx;
	font-size: 34rpx;
	color:#000;
	border-bottom: 1rpx dashed #979797;
}
.title-block:before{
    content: " ";
    position: absolute;
    left:10rpx;
    top:50%;
    width:22rpx;
    height:40rpx;
    background:#5394ff;
    border-radius:8rpx;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleX(0.5) translateY(-50%); 
    transform: scaleX(0.5) translateY(-50%);
}
.textBox_grey{
	padding:3rpx 20rpx;
	font-size:26rpx;
	color:#7a7a7a;
	letter-spacing:-0.63rpx;
	border:2rpx solid #7a7a7a;
	border-radius:100rpx;
}
.wrap-line2{
	justify-content: flex-start;
	padding:20rpx 10rpx 0;
	font-size:28rpx;
	color:#ff8801;
}
.wrap-line3{
	justify-content: flex-start;
	padding:20rpx 30rpx 0 10rpx;
}
/*服务包*/
.bottomBox{
	position: relative;
	margin:20rpx 25rpx 25rpx;
	padding:22rpx 30rpx 15rpx;
	background:#fff;
	box-shadow:4rpx 4rpx 10rpx 0rpx rgba(0,0,0,0.06);
	border-radius:20rpx;
	overflow:hidden;
}
.patch-right_org{
    width:179rpx;
    height: 200rpx;
    position: absolute;
    right:-30rpx;
    bottom:118rpx;
    background-color:#fba559;
    border-radius: 50%;
}
.patch-text_org{
	position: absolute;
	top:20rpx;
	right: 30rpx;
	color: #fff; 
	font-size: 30rpx;
}
.patch-right_grey{
    width:179rpx;
    height: 200rpx;
    position: absolute;
    right:-30rpx;
    bottom:118rpx;
    background:#f4f4f4;
    border-radius: 50%;
}
.patch-text_grey{
	position: absolute;
	top:20rpx;
	right: 30rpx;
	font-size:30rpx;
	color:#b2b2b2;
}