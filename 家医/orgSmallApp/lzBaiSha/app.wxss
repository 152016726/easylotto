/*模态窗*/
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 900;
}
.modal-dialog {
  position: fixed;
  z-index: 5000;
  top: 50%;
  left:50%;
  width: 520rpx;
  padding: 54rpx 45rpx 45rpx 45rpx;
  background-color: #fff;
  border-radius: 11rpx;
  -webkit-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
  text-align: center;
  overflow: hidden; 
}
.modal-title{
  font-size: 36rpx;
  color: #3f3f3f;
  margin-bottom: 74rpx;
}
.modal-btn{
  display: flex;
  justify-content: space-between;
}
.modal-btn button{
  margin-left: 0;
  margin-right: 0;
}


