/**app.wxss**/
@import 'resources/weui.wxss';
@import 'resources/ico.wxss';
@import 'resources/lzico.wxss';
page{
  background-color: #f3f3f3;
}
.clearfox::after{
    display: block;
    clear: both;
    content: '';
    height: 0px;
}
.clearfox .left {
    float: left;
}
.clearfox .right {
    float: right;
}
scroll-view .load-text {
  padding: 10rpx;
  text-align: center;
  font-size:28rpx;
}
.null-type {
   padding: 60rpx;
   font-size:28rpx;
   text-align: center
}
.form-line .error {
  position:absolute;
  font-size:26rpx;
  color:red;
  bottom:0;
}
/***空页面样式*/
.list-null-box{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  flex-direction: column;
  position:absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  color: #999;
  font-size: 30rpx;
}
/*新闻块*/
.news-wrap{
  margin-top:20rpx;
  background: #fff;
}
.news-wrap .news-wrap_title{
  height:100rpx;
  padding:0 42rpx 0 22rpx;
  font-size:34rpx;
  color:#ff8801;
}
.news-wrap .news-wrap_title .lz-news-ico{margin-right:10rpx;}
.news-wrap .news-item{
  position:relative;
  padding:25rpx 42rpx 25rpx 21rpx;
  font-size:32rpx;
  color:#000000;
}
.news-wrap .news-item .news-item_bd{
  position:relative;
  width:473rpx;
  height:130rpx;
}
.news-wrap .news-item .news-item_bd .news-item_title{
  line-height: 1.2;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
}
.news-wrap .news-item .news-item_bd .fc-time{
  position:absolute;
  bottom:0rpx;
  font-size: 28rpx;
  color: #7a7a7a;
}
.news-wrap .news-image{
  width:176rpx;
  height:130rpx;
  background:#f4f4f4;
  border-radius:10rpx;
  margin-left:38rpx;
}

