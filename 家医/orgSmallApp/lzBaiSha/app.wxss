/*弹窗*/
.yt-wrap .dialog-mask{ 
  width: 100%; 
  height: 100%; 
  position: fixed; 
  top: 0; 
  left: 0; 
  z-index: 1000; 
  background: #000; 
  opacity: 0.5; 
  overflow: hidden; 
} 
.yt-wrap .dialog-box{ 
  position: fixed;
  z-index: 5000;
  top: 50%;
  left:50%;
  width: 640rpx;
  -webkit-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
  text-align: center;
  overflow: hidden; 
} 
.yt-wrap .dialog-title{
  padding:40rpx 50rpx;
  color: #525252;
  font-size: 38rpx;
}
.yt-wrap .dialog-content{
  padding:0 35rpx 35rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
.yt-wrap .wrap-box{ 
  min-height: 440rpx;
  padding-bottom: 10px;
  color:#999;
  font-size: 30rpx;
  overflow-y: scroll; /*超出父盒子高度可滚动*/
}
/*列表无数据*/
.yt-wrap .list-null-box{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  flex-direction: column;
  position: absolute; 
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%); 
  color:#999;
  font-size: 30rpx;
}


