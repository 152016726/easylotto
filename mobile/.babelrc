{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": "9",
          "edge": "13",
          "firefox": "60",
          "chrome": "58",
          "safari": "11.1"
        },
        "corejs": 3,
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      {
        "paths": [{
          "rootPathPrefix": "#",
          "rootPathSuffix": "./common_modules"
        }]
      }
    ]
  ]
}